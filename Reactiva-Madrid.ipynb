    "                  title_font=dict(family='Helvetica', color='#7D6F63'),\n",
    "                  plot_bgcolor = '#131211',\n",
    "                  paper_bgcolor='#131211',\n",
    "                  margin={\"r\":0,\"t\":50,\"l\":0,\"b\":0},\n",
    "                  width=900,\n",
    "                  height=500)\n",
    "\n",
    "figure"
